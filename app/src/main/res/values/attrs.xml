<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StyledView">
        <attr name="typeface" format="string"/>
        <attr name="selectedTypeface" format="string"/>
        <attr name="topBorder" format="boolean"/>
        <attr name="bottomBorder" format="boolean"/>
        <attr name="borderColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="FirmnessControlView">
        <attr name="levelDrawable" format="reference"/>
        <attr name="foregroundDrawable" format="reference"/>
        <attr name="knobDrawable" format="reference"/>
        <attr name="targetMetDotColor" format="reference"/>
        <attr name="targetNotMetDotColor" format="reference"/>
        <attr name="dotRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SimpleTabStrip">
        <attr name="tabLayout" format="reference"/>
        <attr name="tabColor" format="reference"/>
        <attr name="selectedTabColor" format="reference"/>
    </declare-styleable>

    <declare-styleable name="Graph">
        <attr name="xAxisLabelSpace" format="dimension"/>
        <attr name="yAxisLabelSpace" format="dimension"/>
    </declare-styleable>



</resources>